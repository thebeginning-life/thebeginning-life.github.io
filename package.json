    "moment": "^2.29.1",
    "node-sass": "^4.14.1",
    "prismjs": "^1.22.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^5.2.1",
    "react-share": "^2.4.0",
    "react-twitter-widgets": "^1.9.5",
    "react-typography": "^0.16.19",
    "typescript": "^4.1.2",
    "typography": "^0.16.19",
    "typography-theme-stow-lake": "^0.16.19",
